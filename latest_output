create host-interface name lo:0
create host-interface name lo:1
set int ip address host-lo:1 127.0.0.3/8
set interface state host-lo:0 up
set interface state host-lo:1 up
lb conf ip4-src-address 127.0.0.3 buckets 32
lb vip 127.0.0.2/32 encap gre4
lb as 127.0.0.2/32 127.0.0.4
create gre tunnel src 127.0.0.3 dst 127.0.0.4
